{
  "name": "url-shortening-graphql-demo",
  "version": "0.0.0",
  "private": true,
  "main": "jest.config.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "release": "npm run clean && npm run build-app && npm run start",
    "clean": "rm -rf dist && mkdir dist",
    "build": "npm run clean && npm run build-app",
    "build-app": "babel src -s -d dist",
    "dev": "nodemon src/bin/www.js",
    "start": "node src/bin/www.js",
    "test": "cross-env NODE_ENV=test jest src/tests/index.test.js",
    "coverage": "npm run test -- '--coverage'",
    "coverage:coveralls": "npm run coverage && coveralls < coverage/lcov.info",
    "coverage:codacy": "npm run coverage && cp -R coverage .github/workflows/coverage/"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "mongoose": "^5.12.4",
    "morgan": "~1.9.1",
    "nanoid": "^3.1.22"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/register": "^7.11.5",
    "coveralls": "^3.1.0",
    "cross-env": "^10.0.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.4.2",
    "jest-chain": "^1.1.5",
    "jest-extended": "^0.11.5",
    "nodemon": "^2.0.4",
    "supertest": "^4.0.2"
  }
}
